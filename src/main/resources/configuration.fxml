<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxWidth="280.0" minWidth="280.0" prefWidth="280.0" spacing="5.0"
      xmlns="http://javafx.com/javafx/null" fx:controller="fu.mi.fitting.controllers.ConfigurationController">
    <TitledPane animated="false" text="Configuration">
        <content>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
                <tabs>
                    <Tab text="fitting">
                        <content>
                            <VBox prefHeight="200.0" prefWidth="100.0">
                                <children>
                                    <GridPane prefHeight="45.0" prefWidth="238.0">
                                        <columnConstraints>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                        </columnConstraints>
                                        <rowConstraints>
                                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        </rowConstraints>
                                        <children>
                                            <ChoiceBox fx:id="fitterChoice" onAction="#chooseFitter" prefWidth="150.0"
                                                       GridPane.columnIndex="1"/>
                                            <Label text="Fitter:"/>
                                        </children>
                                    </GridPane>
                                    <StackPane prefHeight="150.0" prefWidth="200.0">
                                        <children>
                                            <GridPane fx:id="hyperErlangGrid" prefHeight="150.0" prefWidth="238.0">
                                                <columnConstraints>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                       prefWidth="100.0"/>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                       prefWidth="100.0"/>
                                                </columnConstraints>
                                                <rowConstraints>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                    vgrow="SOMETIMES"/>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                    vgrow="SOMETIMES"/>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                    vgrow="SOMETIMES"/>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                    vgrow="SOMETIMES"/>
                                                </rowConstraints>
                                                <children>
                                                    <Label text="branch"/>
                                                    <Label text="reassignment" GridPane.rowIndex="1"/>
                                                    <Label text="queue optimize" GridPane.rowIndex="2"/>
                                                    <Label text="queue optimize" GridPane.rowIndex="3"/>
                                                    <TextField fx:id="branchText" prefHeight="25.0" prefWidth="79.0"
                                                               text="4" GridPane.columnIndex="1"/>
                                                    <TextField fx:id="reassignText" prefHeight="25.0" prefWidth="79.0"
                                                               text="4" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                                    <TextField fx:id="optimizeText" prefHeight="25.0" prefWidth="79.0"
                                                               text="4" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                                                    <TextField fx:id="invoksText" prefHeight="25.0" prefWidth="79.0"
                                                               text="4" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                                                </children>
                                            </GridPane>
                                            <GridPane fx:id="erlangGrid" prefHeight="150.0" prefWidth="238.0"
                                                      visible="false">
                                                <columnConstraints>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                       prefWidth="100.0"/>
                                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                       prefWidth="100.0"/>
                                                </columnConstraints>
                                                <rowConstraints>
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                    vgrow="SOMETIMES"/>
                                                </rowConstraints>
                                                <children>
                                                    <Label text="max phase"/>
                                                    <TextField fx:id="phaseText" prefHeight="25.0" prefWidth="79.0"
                                                               text="2000" GridPane.columnIndex="1"/>
                                                </children>
                                            </GridPane>
                                        </children>
                                    </StackPane>
                                </children>
                                <padding>
                                    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                                </padding>
                            </VBox>
                        </content>
                    </Tab>
                    <Tab text="cluster">
                        <content>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <children>
                                    <Label text="Rule for Equality"/>
                                    <Label text="Rule for Empty Clusters" GridPane.rowIndex="1"/>
                                    <Label text="Rule for Termination" GridPane.rowIndex="2"/>
                                    <ChoiceBox fx:id="equalityChoice" prefWidth="150.0" GridPane.columnIndex="1"/>
                                    <ChoiceBox fx:id="emptyChoice" prefWidth="150.0" GridPane.columnIndex="1"
                                               GridPane.rowIndex="1"/>
                                    <ChoiceBox fx:id="terminationChoice" prefWidth="150.0" GridPane.columnIndex="1"
                                               GridPane.rowIndex="2"/>
                                </children>
                                <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                </padding>
                            </GridPane>
                        </content>
                    </Tab>
                    <Tab text="chart">
                        <content>
                            <GridPane prefHeight="105.0" prefWidth="278.0">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <children>
                                    <Label text="bins"/>
                                    <TextField fx:id="binsTxt" promptText="number of bins in histogram" text="100"
                                               GridPane.columnIndex="1"/>
                                    <TextField fx:id="pdfPointsTxt" promptText="number of point in pdf chart" text="200"
                                               GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                    <Label text="pdf points" GridPane.rowIndex="1"/>
                                    <Label text="cdf points" GridPane.rowIndex="2"/>
                                    <TextField fx:id="cdfPointsTxt" promptText="number of points in cdf chart"
                                               text="200" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    </TextField>
                                    <Label text="Moments" GridPane.rowIndex="3"/>
                                    <TextField fx:id="maxMomentText" text="3" GridPane.columnIndex="1"
                                               GridPane.rowIndex="3"/>
                                </children>
                                <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                </padding>
                            </GridPane>
                        </content>
                    </Tab>
                </tabs>
            </TabPane>
        </content>
    </TitledPane>
</VBox>
