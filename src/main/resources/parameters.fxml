<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="300.0" minWidth="300.0" prefWidth="300.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fu.mi.fitting.controllers.ParameterController">

    <fx:include source="samples.fxml" />
    <fx:include source="configuration.fxml" />
   <HBox prefHeight="25.0" prefWidth="200.0">
      <children>
          <Button fx:id="fitBtn" mnemonicParsing="false" onAction="#fitBtnAction" prefHeight="25.0" prefWidth="48.0" text="Fit" />
         <Button fx:id="exportBtn" mnemonicParsing="false" onAction="#exportBtnAction" text="Export">
            <HBox.margin>
               <Insets left="90.0" />
            </HBox.margin></Button>
      </children>
   </HBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
