<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxWidth="300.0" minWidth="300.0" prefWidth="300.0"
      spacing="5.0" xmlns="http://javafx.com/javafx/null" fx:controller="fu.mi.fitting.controllers.ParameterController">

    <fx:include source="samples.fxml"/>
    <fx:include source="configuration.fxml"/>
    <Button fx:id="fitBtn" mnemonicParsing="false" onAction="#fitBtnAction" prefHeight="25.0" prefWidth="48.0"
            text="Fit">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
    </Button>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
